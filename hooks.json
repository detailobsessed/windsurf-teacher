{
  "hooks": {
    "post_cascade_response": [
      {
        "command": "PLACEHOLDER_PYTHON PLACEHOLDER_HOOKS_DIR/capture_session.py",
        "show_output": false
      }
    ],
    "post_write_code": [
      {
        "command": "PLACEHOLDER_PYTHON PLACEHOLDER_HOOKS_DIR/capture_session.py",
        "show_output": false
      }
    ],
    "post_run_command": [
      {
        "command": "PLACEHOLDER_PYTHON PLACEHOLDER_HOOKS_DIR/capture_session.py",
        "show_output": false
      }
    ]
  }
}
